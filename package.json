{
  "name": "heroku-langchain",
  "version": "0.4.2",
  "description": "Integrate LangChainJS with Heroku's Managed Inference and Agents (Mia) services.",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/esm/index.d.ts",
  "files": [
    "dist",
    "src",
    "LICENSE",
    "README.md"
  ],
  "exports": {
    ".": {
      "require": {
        "types": "./dist/cjs/index.d.ts",
        "default": "./dist/cjs/index.js"
      },
      "import": {
        "types": "./dist/esm/index.d.ts",
        "default": "./dist/esm/index.js"
      }
    }
  },
  "scripts": {
    "build": "node scripts/build-dual.js",
    "build:esm": "tsc -p tsconfig.json",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "build:docs": "typedoc --out docs src/index.ts",
    "dev": "tsx --watch src/index.ts",
    "test": "pnpm test:unit",
    "test:unit": "node --test --import tsx test/*.test.ts",
    "test:integration": "node --test --import tsx test/integration/*.test.ts",
    "test:watch": "node --test --watch --import tsx test/**/*.test.ts",
    "test:integration:watch": "node --test --watch --import tsx test/integration/*.test.ts",
    "typecheck": "tsc --project tsconfig.typecheck.json",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "prepare": "pnpm run build"
  },
  "keywords": [
    "langchain",
    "heroku",
    "ai",
    "llm",
    "mia"
  ],
  "author": "Juli√°n Duque <jduque@heroku.com>",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/julianduque/heroku-langchain.git"
  },
  "bugs": {
    "url": "https://github.com/julianduque/heroku-langchain/issues"
  },
  "homepage": "https://github.com/julianduque/heroku-langchain#readme",
  "engines": {
    "node": ">=20.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@langchain/core": "^1.0.0",
    "@langchain/langgraph": "^1.0.0",
    "langchain": "^1.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@langchain/community": "^1.0.0",
    "@langchain/mcp-adapters": "^1.0.0",
    "@modelcontextprotocol/sdk": "^1.22.0",
    "@types/node": "^24.10.1",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "globals": "^16.5.0",
    "prettier": "^3.6.2",
    "tsx": "^4.20.6",
    "typedoc": "^0.28.14",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.47.0",
    "zod": "^4.1.12"
  },
  "packageManager": "pnpm@10.11.0+sha512.6540583f41cc5f628eb3d9773ecee802f4f9ef9923cc45b69890fb47991d4b092964694ec3a4f738a420c918a333062c8b925d312f42e4f0c263eb603551f977"
}
